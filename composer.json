{
    "name": "reedware/nova-value-toggle",
    "description": "A Laravel Nova field used to toggle fields based on the values of other fields.",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "value",
        "toggle"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": ">=5.1",
        "illuminate/contracts": ">=5.1"
    },
    "autoload": {
        "psr-4": {
            "Reedware\\NovaValueToggle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Reedware\\NovaValueToggle\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
